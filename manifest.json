{
   "background": {
      "scripts": [
         "background.js"
      ]
   },
   "description": "Access https://youtube.com/tv from a regular browser. No ads in this extension.",
   "icons": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "64": "icons/icon-64.png",
      "128": "icons/icon-128.png"
   },
   "manifest_version": 2,
   "name": "YouTube TV",
   "permissions": [
      "*://*.youtube.com/tv*",
      "webRequest",
      "webRequestBlocking"
   ],
   "browser_action": {},
   "short_name": "youtube-tv",
   "version": "1.0.0"
}